#include <kipr/wombat.h>

int main()
{
    printf("Hello World\n");
    
    enable_servos();
    
    set_servo_position(2, 2047);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(2, 828);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(3, 1654);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(3, 0);
    msleep(500);
    ao();
    msleep(1000);
    
    motor(0,100);
    motor(3,100);
    msleep(1000);
    
    
    set_servo_position(2, 754);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(3, 2047);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(2, 790);
    msleep(500);
    ao();
    msleep(1000);
    
    motor(0,-25);
    motor(3,-100);
    msleep(2500);
    
    motor(0,100);
    motor(3,100);
    msleep(500);
    
    ao();
    msleep(1000);
    
    set_servo_position(3, 0);
    msleep(500);
    ao();
    msleep(1000);
    
     motor(0,-100);
    motor(3,-100);
    msleep(1000);
    
    motor(0,-100);
    motor(3,100);
    msleep(850);
    
    
    
    motor(0,100);
    motor(3,100);
    msleep(150);
    
    ao();
    msleep(1000);
    
    set_servo_position(3, 2047);
    msleep(500);
    ao();
    msleep(1000);
    
    set_servo_position(2, 888);
    msleep(500);
    ao();
    msleep(1000);
    
    motor(0,-100);
    motor(3,-100);
    msleep(3000);
    
        ao();
    msleep(1000);
    
    motor(0,100);
    motor(3,0);
    msleep(600);
    
    ao();
    msleep(1000);
    set_servo_position(3, 0);
    msleep(500);
    ao();
    msleep(500);
    
    return 0;
}
